  },
  "scripts": {
    "start": "vite",
    "sideload": "office-addin-debugging start manifest_local.xml desktop --app outlook",
    "validate-manifest": "office-toolbox validate -m manifest_local.xml",
    "install-certs": "office-addin-dev-certs install",
    "uninstall-certs": "office-addin-dev-certs uninstall",
    "verify-certs": "office-addin-dev-certs verify",
    "build": "vite build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "test": "vitest --coverage",
    "e2e": "vitest run --config vite.e2e.config.mts",
    "e2e:watch": "vitest --watch --config vite.e2e.config.mts"
  }
